# Exchange API Keys Configuration
# Copy this file to exchanges.yaml and fill in your API keys
# IMPORTANT: Add exchanges.yaml to .gitignore!

bingx:
  api_key: 'YOUR_BINGX_API_KEY'
  secret: 'YOUR_BINGX_SECRET'
